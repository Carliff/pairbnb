<table class="table table-condensed"> 
			<thead> 
				<tr>   
					<th>Listing Name</th> 
					<th>Price</th>
					<th>City</th>
				</tr> 
			</thead> 
			<tbody> 
				<% @listings.each do |listing| %> 
				<tr> 
					<td><%= link_to listing.name, listing_path(listing) %></td> 
					<td><%= listing.price %></td>
					<td><%= listing.city %></td> 
					<td><%= link_to "edit", edit_listing_path(listing) %></td> 
				</tr> 
				<% end %>
		 </tbody> 
	 </table>