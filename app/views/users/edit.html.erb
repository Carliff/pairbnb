<h1>User#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

  <%= form_for @user do |form| %>

    <div class="text-field">
	  <%= form.label :first_name %>
	  <%= form.text_field :first_name %>
		</div>

		<div class="text-field">
    <%= form.label :email %>
    <%= form.text_field :email, type: 'email' %>
  	</div>

		<div class="submit-field">
    <%= form.submit %>
  	</div>

  <% end %>
