<table class="table table-condensed"> 
			<thead> 
				<tr>   
					<th>Listing Name</th> 
					<th>Date</th>
					
				</tr> 
			</thead> 
			<tbody>
				<% purchased.each do |purchase| %> 
				<tr> 
					<td><%= link_to purchase.listing.name, pickup_path(purchase.guid) %></td> 
					<td><%= time_ago_in_words(purchase.created_at) %> ago 
				</tr> 
				<% end %>
		 </tbody> 
	 </table>